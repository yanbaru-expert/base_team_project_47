<h1>ユーザー 一覧</h1>
<%= link_to  "投稿画面へ", new_user_path %>
<% @users.each do |user| %>
  <p><%= user.name %>
    <%= link_to  "詳細", users_path(user) %>
    <%= link_to  "編集", edit_user_path(user) %>
    <%= link_to  "削除", users_path(user), method: :delete, date: {confirm: "削除しますか？"} %>
  </p>
<% end %>
